{
    "name": "hiqdev/php-ip-tools",
    "description": "A library based on rlanvin/php-ip for IP addresses calculations",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "rlanvin/php-ip": "^3.0.0",
        "symfony/polyfill-php80": "^v1.25"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.22",
        "infection/infection": "^0.25.5",
        "roave/infection-static-analysis-plugin": "^1.13"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "hiqdev\\IpTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hiqdev\\IpTools\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "SilverFire - Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
